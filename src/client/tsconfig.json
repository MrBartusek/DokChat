{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./public/js/",
    "noImplicitAny": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "lib": [
      "es2015",
      "dom"
    ],
    "typeRoots": [
      "./node_modules/@types",
      "./types"
    ]
  },
  "include": [
    "."
  ],
}